[
  {
    $addFields: {
      filtro_inicio_utc: { $dateAdd: { startDate: {
            {data_inicio
            }
          }, unit: "hour", amount: 5
        }
      },
      filtro_fim_utc: { $dateAdd: { startDate: {
            {data_fim
            }
          },    unit: "hour", amount: 29
        }
      }
    }
  },
  {
    $match: {
      $expr: {
        $and: [
          { $gte: [
              "$createdAt",
              "$filtro_inicio_utc"
            ]
          },
          { $lt: [
              "$createdAt",
              "$filtro_fim_utc"
            ]
          }
        ]
      }
    }
  },
  { $project: { pares: { $objectToArray: "$journeyEvaluations"
      }
    }
  },
  { $unwind: "$pares"
  },
  {
    $match: {
      "pares.k": {
        $in: [
          "Custo benef√≠cio",
          "Variedade",
          "Tempo de espera",
          "Atendimento do Caixa",
          "Formas de Pagamento",
          "Atendimento do Vendedor"
        ]
      }
    }
  },
  { $addFields: { valorNum: { $toDouble: "$pares.v"
      }
    }
  },
  {
    $match: {
      $expr: {
        $and: [
          { $gte: [
              "$valorNum",
              1
            ]
          },
          { $lte: [
              "$valorNum",
              5
            ]
          }
        ]
      }
    }
  },
  {
    $group: {
      _id: "$pares.k",
      soma_scores: { $sum: "$valorNum"
      },
      respostas: { $sum: 1
      }
    }
  },
  {
    $project: {
      _id: 0,
      journey: "$_id",
      csat_medio: {
        $round: [
          { $divide: [
              "$soma_scores",
              "$respostas"
            ]
          },
          2
        ]
      },
      respostas: 1
    }
  },
  { $sort: { journey: 1
    }
  }
]